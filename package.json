    "clsx": "^2.1.0",
    "lucide-react": "0.311.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.3",
